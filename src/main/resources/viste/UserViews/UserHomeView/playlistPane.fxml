<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="522.0" prefWidth="192.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.univaq.disim.oop.spacemusicunify.controller.usercontrollers.PlaylistPaneController">
   <children>
      <HBox prefHeight="522.0" prefWidth="192.0">
         <children>
            <VBox id="vbox" alignment="BOTTOM_LEFT" prefHeight="522.0" prefWidth="180.0">
               <children>
                  <TableView fx:id="playlistView" prefHeight="480.0" prefWidth="195.0">
                     <columns>
                        <TableColumn fx:id="titleColumn" prefWidth="179.0" text="My Playlists" />
                     </columns>
                  </TableView>
                  <Button id="addP" fx:id="addPlaylist" mnemonicParsing="false" onAction="#addNewPlaylist" prefHeight="42.0" prefWidth="189.0" text="+ New Playlist" underline="true">
                     <font>
                        <Font name="Times New Roman" size="21.0" />
                     </font>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
               </children>
            </VBox>
            <Separator orientation="VERTICAL" prefHeight="519.0" prefWidth="12.0" />
         </children>
      </HBox>
   </children>
</AnchorPane>
